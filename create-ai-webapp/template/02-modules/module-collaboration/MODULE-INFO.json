{
  "name": "module-collaboration",
  "displayName": "Collaboration Module",
  "version": "5.0.0",
  "description": "Real-time collaborative editing with CRDT synchronization, user presence tracking, and version control",
  "priority": "P2",
  "phase": 3,
  "extracted": "2025-10-10",
  "status": "completed",
  "stats": {
    "files": 11,
    "codeLines": 2087,
    "testCases": 56,
    "testCoverage": "85%+",
    "documentation": 650
  },
  "features": [
    "Real-time collaborative editing",
    "CRDT-based conflict-free synchronization",
    "WebSocket real-time communication",
    "User presence tracking (online/offline, cursor, selection, typing)",
    "Automatic conflict resolution",
    "Change history tracking",
    "Version control",
    "Multi-session support",
    "React Context Provider integration",
    "UI components (cursor overlay, user list, sync status)"
  ],
  "dependencies": {
    "yjs": "^13.6.0",
    "y-websocket": "^1.5.0",
    "y-protocols": "^1.0.6"
  },
  "database": {
    "models": [
      "CollaborationSession",
      "DocumentChange",
      "DocumentVersion"
    ],
    "supported": [
      "PostgreSQL",
      "MySQL",
      "MongoDB",
      "SQLite"
    ]
  },
  "files": {
    "core": [
      "lib/collaboration/collaborative-editing.ts.template",
      "lib/collaboration/presence.ts.template"
    ],
    "components": [
      "components/CollaborationProvider.tsx.template"
    ],
    "types": [
      "types/collaboration.d.ts.template"
    ],
    "tests": [
      "lib/collaboration/__tests__/collaborative-editing.test.ts.template",
      "lib/collaboration/__tests__/presence.test.ts.template"
    ],
    "docs": [
      "README.md",
      "USAGE-EXAMPLE.md",
      "EXTRACTION-SUMMARY.md",
      "prisma-schema.txt",
      "package.json.snippet"
    ]
  },
  "api": {
    "services": [
      "CollaborativeEditingService",
      "PresenceService"
    ],
    "hooks": [
      "useCollaboration"
    ],
    "components": [
      "CollaborationProvider",
      "CursorOverlay",
      "ActiveUsersList",
      "SyncStatusIndicator"
    ]
  },
  "requirements": {
    "external": [
      "WebSocket server (y-websocket-server)"
    ],
    "environment": [
      "NEXT_PUBLIC_WEBSOCKET_URL"
    ]
  },
  "integrations": {
    "database_adapter": true,
    "nextjs_app_router": true,
    "typescript_strict": true,
    "jest_testing": true
  }
}
